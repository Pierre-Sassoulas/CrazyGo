<Window x:Class="CrazyGo.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CrazyGo.WPF"
        xmlns:View = "clr-namespace:CrazyGo.WPF.View"
        mc:Ignorable="d"
        Title="MainWindow"
        WindowState="Maximized">
    <Grid>
        <View:GobanView x:Name="GobanViewControl" Loaded = "GobanViewControl_Loaded" BorderBrush="Green" BorderThickness="5"/>
    </Grid>
</Window>
